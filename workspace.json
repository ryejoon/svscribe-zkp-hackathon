{
  "version": 2,
  "projects": {
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"],
            "generatePackageJson": true
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "deploy": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "nx build api",
              "docker build -f ./apps/api/Dockerfile . -t api"
            ],
            "parallel": false
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/api/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/api"],
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "html": {
      "root": "apps/html",
      "sourceRoot": "apps/html/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/html",
            "main": "apps/html/src/main.ts",
            "tsConfig": "apps/html/tsconfig.app.json",
            "assets": ["apps/html/src/assets"],
            "generatePackageJson": true
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/html/src/environments/environment.ts",
                  "with": "apps/html/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "deploy": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "nx build html",
              "docker build -f ./apps/html/Dockerfile . -t html"
            ],
            "parallel": false
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "html:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/html/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/html"],
          "options": {
            "jestConfig": "apps/html/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "zkksp-prover": {
      "projectType": "application",
      "root": "apps/zkksp-prover",
      "sourceRoot": "apps/zkksp-prover/src",
      "prefix": "zkp-hackathon",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/zkksp-prover",
            "index": "apps/zkksp-prover/src/index.html",
            "main": "apps/zkksp-prover/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "apps/zkksp-prover/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/zkksp-prover/src/favicon.ico",
              "apps/zkksp-prover/src/assets"
            ],
            "styles": ["apps/zkksp-prover/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "zkksp-prover:build:production"
            },
            "development": {
              "browserTarget": "zkksp-prover:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "zkksp-prover:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/zkksp-prover/**/*.ts",
              "apps/zkksp-prover/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
          "options": {
            "jestConfig": "apps/zkksp-prover/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "todos": {
      "root": "libs/todos",
      "sourceRoot": "libs/todos/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/todos/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/todos"],
          "options": {
            "jestConfig": "libs/todos/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
