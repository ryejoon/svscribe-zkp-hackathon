{
  "version": 2,
  "projects": {
    "zkksp-prover-backend": {
      "root": "apps/zkksp-prover-backend",
      "sourceRoot": "apps/zkksp-prover-backend/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/zkksp-prover-backend",
            "main": "apps/zkksp-prover-backend/src/main.ts",
            "tsConfig": "apps/zkksp-prover-backend/tsconfig.app.json",
            "assets": [
              "apps/zkksp-prover-backend/src/assets"
            ],
            "generatePackageJson": true
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/zkksp-prover-backend/src/environments/environment.ts",
                  "with": "apps/zkksp-prover-backend/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "deploy": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "nx build zkksp-prover-backend",
              "docker build -f ./apps/zkksp-prover-backend/Dockerfile . -t zkksp-prover-backend"
            ],
            "parallel": false
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/zkksp-prover-backend/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/zkksp-prover-backend"
          ],
          "options": {
            "jestConfig": "apps/zkksp-prover-backend/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "html": {
      "root": "apps/html",
      "sourceRoot": "apps/html/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/html",
            "main": "apps/html/src/main.ts",
            "tsConfig": "apps/html/tsconfig.app.json",
            "assets": [
              "apps/html/src/assets"
            ],
            "generatePackageJson": true
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/html/src/environments/environment.ts",
                  "with": "apps/html/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "deploy": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "nx build html",
              "docker build -f ./apps/html/Dockerfile . -t html"
            ],
            "parallel": false
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "html:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/html/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/html"
          ],
          "options": {
            "jestConfig": "apps/html/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "todos": {
      "root": "libs/todos",
      "sourceRoot": "libs/todos/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/todos/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/todos"
          ],
          "options": {
            "jestConfig": "libs/todos/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "zkksp-prover-angular": {
      "projectType": "application",
      "root": "apps/zkksp-prover-angular",
      "sourceRoot": "apps/zkksp-prover-angular/src",
      "prefix": "zkp-hackathon",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/zkksp-prover-angular",
            "index": "apps/zkksp-prover-angular/src/index.html",
            "main": "apps/zkksp-prover-angular/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "apps/zkksp-prover-angular/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/zkksp-prover-angular/src/favicon.ico",
              "apps/zkksp-prover-angular/src/assets"
            ],
            "styles": [
              "apps/zkksp-prover-angular/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "zkksp-prover-angular:build:production"
            },
            "development": {
              "browserTarget": "zkksp-prover-angular:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "deploy": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "nx build zkksp-prover-angular",
              "docker build -f ./apps/zkksp-prover-angular/Dockerfile . -t zkksp-prover-angular"
            ],
            "parallel": false
          }
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "zkksp-prover-angular:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/zkksp-prover-angular/**/*.ts",
              "apps/zkksp-prover-angular/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "{workspaceRoot}/coverage/{projectRoot}"
          ],
          "options": {
            "jestConfig": "apps/zkksp-prover-angular/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "zkksp-verifier-backend": {
      "root": "apps/zkksp-verifier-backend",
      "sourceRoot": "apps/zkksp-verifier-backend/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/zkksp-verifier-backend",
            "main": "apps/zkksp-verifier-backend/src/main.ts",
            "tsConfig": "apps/zkksp-verifier-backend/tsconfig.app.json",
            "assets": [
              "apps/zkksp-verifier-backend/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/zkksp-verifier-backend/src/environments/environment.ts",
                  "with": "apps/zkksp-verifier-backend/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "deploy": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "nx build zkksp-verifier-backend",
              "docker build -f ./apps/zkksp-verifier-backend/Dockerfile . -t zkksp-verifier-backend"
            ],
            "parallel": false
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "zkksp-verifier-backend:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/zkksp-verifier-backend/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/zkksp-verifier-backend"
          ],
          "options": {
            "jestConfig": "apps/zkksp-verifier-backend/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "defaultProject": "zkksp-prover-backend",
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  }
}
